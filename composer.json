{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "doctrs/sonata-import-bundle": "^1.3",
        "friendsofsymfony/rest-bundle": "^2.7",
        "gesdinet/jwt-refresh-token-bundle": "^0.6.2",
        "guzzlehttp/guzzle": "^6.5",
        "jms/serializer-bundle": "^3.5",
        "kunicmarko/importer": "^0.2.0",
        "kunicmarko/sonata-importer-bundle": "^0.1.1",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nelmio/api-doc-bundle": "^3.6",
        "nelmio/cors-bundle": "^2.0",
        "ocramius/package-versions": "1.4.2",
        "sensio/framework-extra-bundle": "^5.5",
        "sonata-project/admin-bundle": "^3.63",
        "sonata-project/doctrine-orm-admin-bundle": "^3.15",
        "sonata-project/user-bundle": "^4.5",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/acl-bundle": "^2.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/dom-crawler": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/event-dispatcher": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/security-core": "4.4.*",
        "symfony/security-csrf": "4.4.*",
        "symfony/security-guard": "4.4.*",
        "symfony/security-http": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/maker-bundle": "^1.14"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
